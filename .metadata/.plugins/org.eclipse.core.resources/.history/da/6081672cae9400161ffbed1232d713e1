package room;
import static org.junit.Assert.*;
import org.junit.*;
public class PointTest {

	Point p = new Point(1,2);
	
	@Test
	public void testGetPoints(){
		assertEquals(1, p.getX());
		assertEquals(2,p.getY());
		
	}
	
	public void testToString(){
		
		assertEquals("1 2",p.ToString());
	}
	
	
}
