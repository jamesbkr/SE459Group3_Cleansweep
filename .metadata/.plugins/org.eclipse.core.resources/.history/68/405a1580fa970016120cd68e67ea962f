package room;
import static org.junit.Assert.*;
import org.junit.*;
public class roomTest {
	
	Room r = new Room();
	
	
	@Test
	public void updateRoomTest(){
		r.getRoom().put(new Point(0,0),new RoomStatus(ThingsInRoom.BASE,CleanRoom.CLEAN));
		
		assertTrue(r.getRoom().keySet().size() == 1);
		
	}
	
	
	
	

}
