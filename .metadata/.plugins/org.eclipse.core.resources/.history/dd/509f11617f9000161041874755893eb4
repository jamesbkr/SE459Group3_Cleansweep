package room;

import java.util.Arrays;

public class Point {
	int x;
	int y;
	
	public Point(int a, int b){
		x = a;
		y = b;
	}
	
	
	public int getX(){
		return x;
	}
	
	public int getY(){
		return y;
	}
	
	public String ToString(){
		return "point is " +x + " "+y;		
	}
	
	
	@Override
	public int hashCode(){
		return Arrays.hashCode(new Object[]{new Character(x),new Character(y)});
	}
	
}
